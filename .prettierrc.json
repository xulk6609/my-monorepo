{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 160,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/.*$",
    "^@/constants/.*$",
    "^@/apis/.*$",
    "^@/composables/.*$",
    "^@/stores/.*$",
    "^@/utils/.*$",
    "^@/i18n/.*$",
    "^@/assets/.*$",
    "^@/.*$",
    "^\\.\\./.*$",
    "^\\./.*$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
